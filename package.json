{
  "name": "cookiecutter",
  "version": "0.0.3",
  "description": "Pretty CLI for quickly creating boilerplate files based on templates you provide.",
  "bin": {
    "cookiecutter": "src/index.js"
  },
  "author": "mattvagni",
  "license": "MIT",
  "dependencies": {
    "colors": "1.1.2",
    "fs-extra": "3.0.1",
    "inquirer": "3.0.6"
  },
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "jest": "^20.0.1"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "testPathIgnorePatterns": [
      "test-templates-output"
    ]
  }
}
